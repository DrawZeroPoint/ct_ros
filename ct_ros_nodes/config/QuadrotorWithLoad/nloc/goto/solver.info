timeHorizon 1.0

nloc
{
	integrator EulerCT
	useSensitivityIntegrator false
	discretization Forward_euler
	timeVaryingDiscretization false    
	dt 0.005
    K_sim 1
    K_shot 1
    epsilon 1e-6
	max_iterations 50
    fixedHessianCorrection false
    recordSmallestEigenvalue false
    min_cost_improvement 1e-5
    meritFunctionRho 0.0
    maxDefectSum 1e-2
    nThreads 1
    nThreadsEigen 1
    locp_solver GNRICCATI_SOLVER
    nlocp_algorithm GNMS
    printSummary true
    debugPrint false
    logToMatlab false
    loggingPrefix ""
  
    line_search
	{
		type NONE ;
		adaptive false ;
		maxIterations 10 ;
		alpha_0 1.0 ;
		n_alpha 0.5;
		debugPrint false
	}
}