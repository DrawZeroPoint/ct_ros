termTaskSpace_final
{
	name "final cost taskspace"
	kind "quadratic"   
	type 1              ; 0 = intermediate, 1 = final

	weights
	{
		eeId	0
		
		evalControlDerivatives 0
		
		Q_rot 10.0
		
		; desired xyz euler angles
		eulerXyz_des
		{
			(0,0)  0.0
			(1,0)  0.0
			(2,0)  0.0
		}
			
		; state weighting
		Q_pos
		{
			scaling 5.0

			; position weighting          
			(0,0)  1
			(1,1)  1
			(2,2)  1
		}

		x_des
		{
			; des position        
			(0,0)   0.25
			(1,0)   -0.25      
			(2,0)   0.5
		}
	}
}


